@model List<SisEventos.Models.Estacionamento>
@{
    ViewData["Title"] = "Home";
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
<link rel="stylesheet" href="~/css/StyleSheet.css" />
<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img class="first-slide" src="../../estacionamento1.png" alt="First slide">
            <div class="container">
                <div class="carousel-caption">
                    <h1>Aonde você vai tem</h1>
                    <p>Seja bem vindo à maior rede de estacionamento do Sul do Brasil.</p>
                </div>
            </div>
        </div>
        <div class="item">
            <img class="second-slide" src="../../estacionamento.png" alt="Second slide">
            <div class="container">
                <div class="carousel-caption">
                    <h1>Aonde você vai tem</h1>
                    <p>Seja bem vindo à maior rede de estacionamento do Sul do Brasil.</p>
                </div>
            </div>
        </div>
        <div class="item">
            <img class="third-slide" src="../../estacionamento2.jpg" alt="Third slide">
            <div class="container">
                <div class="carousel-caption">
                    <h1>Aonde você vai tem</h1>
                    <p>Seja bem vindo à maior rede de estacionamento do Sul do Brasil.</p>
                    <p><a class="btn btn-default" href="../../estacionamentos" role="button">Encontre um estacionamento</a></p>
                </div>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
    </br>
    </br>
<div class="jumbotron">
    <h1>Conheça a história da AVJ Parking</h1>
    <p><center><img src="../../mapa.png" /></center> </p>
    <p class="lead">Em 2018 surgia em Joinville uma empresa de estacionamentos com uma proposta inovadora. Além de serviços padronizados e atendimento diferenciado, a AVJ Parking apresentava algo incomum para a época: a preocupação em construir uma marca forte. Era o início de uma história de sucesso.</p>
</div>
