@model SisEventos.Models.Cupom
@{
    ViewData["Title"] = "Detail";
}

<link rel="stylesheet" href="~/css/tables.css" />

<h2>Cupom</h2>
<div class="evento-detail">
    <table class="greyGridTable">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Cupom</th>
                <th scope="col">Porcentagem de desconto</th>
                <th scope="col">Estacionamento</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">@Model.Id</th>
                <td>@Model.Nome</td>
                <td>@Model.Descricao</td>
                <td>@Model.estacionamento.Nome</td>
            </tr>
        </tbody>
    </table>
</div>
