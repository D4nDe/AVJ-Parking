@model SisEventos.ViewModels.FinanciaVM
@{
    ViewData["Title"] = "Alteração de relátorio";
}
<h1>Alteração de relátorio</h1>
<div class="row">
    <form asp-controller="Financias"
          asp-action="Edit"
          enctype="multipart/form-data"
          method="post"
          class="form-horizontal">
        <div class="form-group">
            <label class="col-sm-2 control-label" asp-for="Dates"></label>
            <div class="col-sm-10">
                <input class="form-control" asp-for="Dates" />
                <small class="text-danger" asp-validation-for="Data"></small>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label" asp-for="Total"></label>
            <div class="col-sm-10">
                <input class="form-control" asp-for="Total" />
                <small class="text-danger" asp-validation-for="Total"></small>
            </div>
        </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <a asp-area="Admin"
                   asp-controller="Financias"
                   asp-action="Index"
                   class="btn btn-default">Cancelar</a>
                <button type="submit"
                        class="btn btn-success">
                    Salvar
                </button>
            </div>
        </div>
    </form>
</div>
